        <div class="row">
            <div class="col-md-10 col-12 mx-auto">
                <div class="row">
                    <!-- left header -->
                    <div class="col-md-6 col-12 main_header_left">
                        <p>Welcome to Garima's Art Gallery</p>
                        <h1>These are my few <span>Arts.</span></h1> 
                        <h1>If you wanna see more of these. Visit my <span>Instagram</span> page. there you can see my all <span>Arts</span>.</h1> 
                        <a href="https://www.instagram.com/garima.arts/"><button>View Page</button></a>                   
                    </div>
                    <!-- right header  -->
                    <div class="col-md-5 col-12 main_header_center">
                        <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
                            <div class="carousel-inner">
                              <div class="carousel-item active">
                                 <img class="w-100" src="Images/2.jpg" alt="First slide">
                              </div>
                              <div class="carousel-item">
                                <img class="w-100" src="Images/1.jpg" alt="Second slide">
                              </div>
                              <div class="carousel-item">
                                <img class="w-100" src="Images/3.jpg" alt="Third slide">
                              </div>
                              <div class="carousel-item">
                                <img class="w-100" src="Images/4.jpg" alt="Third slide">
                              </div>
                              <div class="carousel-item">
                                <img class="w-100" src="Images/5.jpg" alt="Third slide">
                              </div>
                              <div class="carousel-item">
                                <img class="w-100" src="Images/6.jpg" alt="Third slide">
                              </div>
                              <div class="carousel-item">
                                <img class="w-100" src="Images/7.jpg" alt="Third slide">
                              </div>
                               <div class="carousel-item">
                                <img class="w-100" src="Images/8.jpg" alt="Third slide">
                              </div>
                              <div class="carousel-item">
                                <img class="w-100" src="Images/9.jpg" alt="Third slide">
                              </div>
                              <div class="carousel-item">
                                <img class="w-100" src="Images/10.jpg" alt="Third slide">
                              </div>
                            </div>
                            <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                              <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                              <span class="sr-only">Previous</span>
                            </a>
                            <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                              <span class="carousel-control-next-icon" aria-hidden="true"></span>
                              <span class="sr-only">Next</span>
                            </a>
                        </div>                        
                    </div>
                </div>
            </div>
        </div>
    </div>   
    
    <!-- footer code  -->
    {{>Footer}}
